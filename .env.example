# Quantara Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Application Configuration
VITE_APP_NAME=Quantara
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Admin Configuration
VITE_ADMIN_EMAIL_DOMAIN=quantara.dev
VITE_MIN_PASSWORD_LENGTH=8

# Feature Flags
VITE_ENABLE_ADMIN_SETUP=true
VITE_ENABLE_CLIENT_APPLICATIONS=true
VITE_ENABLE_MEMBER_INVITES=true

# Security Configuration
VITE_SESSION_TIMEOUT=3600000
VITE_MAX_LOGIN_ATTEMPTS=5